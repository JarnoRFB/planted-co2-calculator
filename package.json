{
  "name": "planted-co2-calculator",
  "version": "1.0.0",
  "description": "CO2 calculator for planted.green",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "tsc && node dist/server.js",
    "test": "jest --coverage --verbose",
    "clean": "rm -rf dist/",
    "data": "tsc && node dist/dataAcquisition/retrieveElectricity.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JarnoRFB/planted-co2-calculator.git"
  },
  "keywords": [
    "carbon"
  ],
  "author": "rbusche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JarnoRFB/planted-co2-calculator/issues"
  },
  "homepage": "https://github.com/JarnoRFB/planted-co2-calculator#readme",
  "dependencies": {
    "@tmrow/bloom-contrib": "0.0.27",
    "body-parser": "^1.19.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "fp-ts": "^2.9.3",
    "io-ts": "^2.2.13",
    "io-ts-reporters": "^1.2.2",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.23",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "jest": "^26.6.3",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "preset": "ts-jest",
    "testTimeout": 10000,
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}