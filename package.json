{
  "name": "planted-co2-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "test:cov": "jest --coverage --verbose",
    "clean": "rm -rf dist/",
    "data:electricity": "tsc --module 'CommonJS' && node dist/dataAcquisition/retrieveElectricity.js",
    "data:heating": "tsc --module 'CommonJS' && node dist/dataAcquisition/retrieveHeating.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JarnoRFB/planted-co2-calculator.git"
  },
  "keywords": [
    "carbon"
  ],
  "author": "rbusche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JarnoRFB/planted-co2-calculator/issues"
  },
  "homepage": "https://github.com/JarnoRFB/planted-co2-calculator#readme",
  "dependencies": {
    "@tmrow/bloom-contrib": "^0.0.27",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.32",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.14",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.23",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.14",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.5",
    "eslint": "^6.7.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "jest": "^26.6.3",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "~3.9.3",
    "vue-cli-plugin-element-plus": "~0.0.13"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "preset": "ts-jest",
    "testTimeout": 10000,
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
